<%include ../partials/header.ejs %>

<!--contents goes here-->
<!--div.container>div.row>div.col-md-offset-3.col-md-6-->
<div class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-6">
      <h1><%= title %></h1>

     
      <br />
      <!--table.table.table-bordered.table-striped.table.hover>tr>th*5.text-center-->
      <table class="table table-bordered table-striped hover">
        <!-- Header row-->
        <tr>
          <th class="text-center">First Name</th>
          <th class="text-center">Last Name</th>
          <th class="text-center">Email</th>
          <th class="text-center">Contact Number</th>
          <th class="text-center">Message</th>
          <th class="text-center"></th>
          
        </tr>
        <!--Templet Row-->
        <% for(let count = 0; count < messageList.length; count++) { %>
        <tr>
          <td class="text-center">
            <%= messageList[count].first_name %>
          </td>
          <td class="text-center">
            <%= messageList[count].last_name %>
          </td>
          <td class="text-center">
            <%= messageList[count].age %>
          </td>

          

          <td class="text-center">
            <a
              href="/contact/delete/<%= messageList[count]._id %>"
              class="btn btn-danger"
              >Delete
            </a>
          </td>
        </tr>
        <% } %>
      </table>
    </div>
  </div>
</div>

<%include ../partials/footer.ejs %>
